! Xft settings
Xft.dpi: 96
Xft.antialias: true 
Xft.hinting: true 
Xft.rgba: rgb 
Xft.autohint: true 
Xft.hintstyle: hintfull 
Xft.lcdfilter: lcdfilter

! Xcursor settings
Xcursor.size: 16
Xcursor.theme: Breeze

! sxiv settings
! *foreground: rgb:e5/e5/e5 
! *background: rgb:00/00/00
